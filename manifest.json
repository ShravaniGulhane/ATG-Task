{
    "manifest_version": 3,
    "name": "Tab Title Grabber",
    "version": "1.0",
    "description": "Grab and display the current tab's title",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  